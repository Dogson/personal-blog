{
  "name": "blog",
  "description": "Blog using Gatsby, Tailwind CSS and Netlify CMS project",
  "version": "1.0.0",
  "author": "Josef Rousek",
  "dependencies": {
    "autoprefixer": "^9.0.1",
    "cssnano": "^4.0.5",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-netlify-cms": "next",
    "gatsby-plugin-postcss": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-remark-images": "next",
    "gatsby-remark-relative-images": "git+https://github.com/stlk/gatsby-remark-relative-images.git",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "^2.0.8",
    "parcel-bundler": "^1.9.4",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^5.3.0",
    "precss": "^3.1.2",
    "prop-types": "^15.6.0",
    "purgecss-webpack-plugin": "^1.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "tailwindcss": "^0.6.4",
    "uuid": "^3.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "build": "npm run clean && gatsby build",
    "develop": "npm run clean && gatsby develop",
    "serve": "npm run clean && gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "{gatsby-*.js,src/**/*.js}": [
        "prettier --trailing-comma es5 --no-semi --single-quote --write",
        "git add"
      ]
    },
    "ignore": [
      "src/styles/tailwind.js"
    ]
  }
}
